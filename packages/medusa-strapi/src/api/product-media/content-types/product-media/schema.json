{
  "kind": "collectionType",
  "collectionName": "product_medias",
  "info": {
    "singularName": "product-media",
    "pluralName": "product-medias",
    "displayName": "Product Media",
    "description": "To store product related multimedia files"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true,
    "comment": ""
  },
  "pluginOptions": {
    "versions": {
      "versioned": true
    }
  },
  "attributes": {
    "medusa_id": {
      "type": "uid",
      "required":true
    },
    "filename":
    {
      "type":"string",
      "required":true
    },
    "files": {
      "type": "media",
      "multiple": true
    },
    "media_url": {
      "type": "text",
      "multiple": true
    },
    "metadata": {
      "type": "json"
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "product-medias"
    }
  }
}