{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true,
    "comment": ""
  },
  "pluginOptions": {
    "versions": {
      "versioned": true
    }
  },
  "attributes": {
    "medusa_id": {
      "type": "uid"
    },
    "title": {
      "type": "text",
      "required": true,
      "unique": false
    },
    "subtitle": {
      "type": "text"
    },
    "description": {
      "type": "text"
    },
    "handle": {
      "type": "text"
    },
    "is_giftcard": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "draft",
        "proposed",
        "published",
        "rejected"
      ],
      "default": "published",
      "required": true
    },
    "thumbnail": {
      "type": "string"
    },
    "discountable": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "weight": {
      "type": "decimal"
    },
    "product_length": {
      "type": "decimal"
    },
    "width": {
      "type": "decimal"
    },
    "height": {
      "type": "decimal"
    },
    "hs_code": {
      "type": "string"
    },
    "origin_country": {
      "type": "string"
    },
    "mid_code": {
      "type": "string"
    },
    "material": {
      "type": "string"
    },
    "metadata": {
      "type": "json"
    },
    "metafields": {
      "type": "json"
    },
    "product-variants": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-variant.product-variant",
      "mappedBy": "product"
    },
    "product-options": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-option.product-option",
      "mappedBy": "product"
    },
    "images": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::image.image",
      "mappedBy": "product"
    },
    "product-collection": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product-collection.product-collection",
      "inversedBy": "products"
    },
    "product-categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-category.product-category",
      "mappedBy": "products"
    },
    "product-type": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product-type.product-type",
      "inversedBy": "products"
    },
    "product-tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-tag.product-tag",
      "mappedBy": "products"
    },
    "product-medias": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-media.product-media",
      "mappedBy": "product"
    },
    "product-metafield": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product-metafield.product-metafield",
      "inversedBy": "product"
    },
    "product-documents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-document.product-document",
      "mappedBy": "product"
    },
    "product-legal": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product-legal.product-legal",
      "inversedBy": "products"
    },
    "store": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::store.store",
      "inversedBy": "products"
    }
  }
}
