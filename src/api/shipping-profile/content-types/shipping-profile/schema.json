{
  "kind": "collectionType",
  "collectionName": "shipping_profiles",
  "info": {
    "singularName": "shipping-profile",
    "pluralName": "shipping-profiles",
    "displayName": "Shipping Profile",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "medusa_id": {
      "type": "uid",
      "required": true
    },
    "name": {
      "type": "string",
      "required": false
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "default",
        "gift_card",
        "custom"
      ],
      "required": true,
      "default": "default"
    },
    "metadata": {
      "type": "json"
    },
    "shipping_options": {
      "via": "shipping_profile",
      "collection": "shipping-option"
    },
    "products": {
      "via": "shipping_profile",
      "collection": "product"
    }
  }
}
